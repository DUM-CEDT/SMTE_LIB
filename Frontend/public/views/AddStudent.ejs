            </div>
            
            <div></div>
            <input id="Student-Phone" type="text" placeholder="โทรศัพท์">
            
            <button id="submit-button">ยืนยัน</button>
        </div>
    </div>
    <script type="module" src="../JS/AddStudent.js"></script>
